class Solution {
public:
    void moveZeroes(vector<int>& nums) {
        if(nums.size()==1) return ;
        int j=0;
        for(int i=0;i<nums.size();i++){
            if(nums[i]!=0)
                nums[j++]= nums[i];
                }
        for(int i=j;i<nums.size();i++)
            nums[i] = 0;
    }
};

// putting non-zero elements in left part and filling the remaining with zeros
